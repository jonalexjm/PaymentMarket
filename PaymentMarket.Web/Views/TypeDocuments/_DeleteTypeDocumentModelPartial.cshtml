@model PaymentMarket.Core.Entities.TypeDocument

<div class="modal fade" id="addTypeDocument">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="addTypeDocumentLabel">Esta Seguro de eliminar este registro?</h4>
                <button type="button" class="close" data-dismiss="modal">
                    <span>X</span>
                </button>
            </div>
            <div class="modal-body">
                <form action = "/TypeDocuments/Delete">
                    <div class="form-group">
                        <input type="hidden" asp-for="@Model.Id"/>
                        <label asp-for="Id">@Model.Id</label>
                        <label asp-for="Description">@Model.Description</label>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Cerrar</button>
                <button type="button" class="btn btn-success" data-save="modal">Eliminar</button>
            </div>
        </div>
    </div>
</div>

